package linkedlist;

public class MeargeSortOnLinkedList {

	public static void main(String[] args) {
		
		LinkedListM lm=new LinkedListM();
		lm.insert(14);
		lm.insert(65);
		lm.insert(50);
		lm.insert(40);
		lm.insert(40);
		
	}
}

class LinkedListM {

	Node head;
	class Node {

		int data;
		Node next;

		
		Node(int data)
		{
			this.data=data;
			next=null;
		}
		
		
				
	}
	
	void insert(int data)
	{
		if(head==null)
		{
			head=new Node(data);
			
		}
		else
		{
			Node temp=new Node(data);
			head.next=temp;
			
		}
	}
	
	


}