	public static void main(String[] args)throws Exception {
	int arr[]={1,2};
	try{
	System.out.println(arr[2]);
      }
	finally{
		
		System.out.println("Inside finally block");
	}
/*	catch(Exception e)
	{
		
		
	}*/
	
	System.out.println(arr[0]);
}
}
