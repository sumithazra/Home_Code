	 public static void main (String[] args) 
	    {
	        int arr1[] = {1, 2, 3};
	        int arr2[] = {1, 2, 3};
	        if (arr1 == arr2) // Same as arr1.equals(arr2)
	            System.out.println("Same");
	        else
	            System.out.println("Not same");
	        
	        Integer i1=new Integer(10);
	        Integer i2=new Integer(10);
	        
	        if(i1.equals(i2))
	        {
	        	System.out.println("same ");
	        	
	        }
	        else
	        {
	        	System.out.println("not same");
	        }
	        
	    }
	 
	 
}

