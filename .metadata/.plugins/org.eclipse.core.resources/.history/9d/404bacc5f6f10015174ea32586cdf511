package stringExp;

public class StringProgram {

	public int slowLength(String myString) {
	    int i = 0;
	    try {
	        while (true) {
	            myString.charAt(i);
	            i++;
	        }
	    } catch (IndexOutOfBoundsException e) {
	    	System.out.println("Inside catch block");
	       return i;
	    }
	    finally{
	    	System.out.println("Inside finally bolck");
	    }
	    
	}
	
}
