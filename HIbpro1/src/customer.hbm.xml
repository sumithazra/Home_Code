<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
 
 <hibernate-mapping>
 <class name="com.javabrain.test.Customer" table="employee">
 <id name="c_id"  column="id" type="int">
 <generator class="increment" ></generator>
 </id>
 
<property name="c_name" column="customer_name" type="string"></property>
<property name="c_email" column="customer_email" type="string"></property>
<property name="c_phone" column="customer_phone" type="int"></property>
 
 </class>
 
 
 
 
 
 </hibernate-mapping>